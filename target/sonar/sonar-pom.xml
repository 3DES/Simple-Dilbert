<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.mareksebera</groupId>
  <artifactId>dilbert</artifactId>
  <version>1.8</version>
  <packaging>pom</packaging>
  <name>Simple Dilbert Android</name>
  <properties>
    <sonar.jdbc.url>jdbc:mysql://audminlinux.emea.hpqcorp.net:3306/sonar</sonar.jdbc.url>
    <sonar.phase>generate-sources</sonar.phase>
    <sonar.jdbc.password>sonar</sonar.jdbc.password>
    <sonar.host.url>http://audminlinux.emea.hpqcorp.net:9000</sonar.host.url>
    <sonar.dynamicAnalysis>true</sonar.dynamicAnalysis>
    <sonar.jdbc.driverClassName>com.mysql.jdbc.Driver</sonar.jdbc.driverClassName>
    <sonar.jdbc.username>sonar</sonar.jdbc.username>
  </properties>
  <repositories>
    <repository>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>central</id>
      <name>Central Repository</name>
      <url>http://repo.maven.apache.org/maven2</url>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <releases>
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>central</id>
      <name>Central Repository</name>
      <url>http://repo.maven.apache.org/maven2</url>
    </pluginRepository>
  </pluginRepositories>
  <build>
    <sourceDirectory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/src</sourceDirectory>
    <scriptSourceDirectory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/src/main/scripts</scriptSourceDirectory>
    <testSourceDirectory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/src/test/java</testSourceDirectory>
    <outputDirectory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/target/classes</outputDirectory>
    <testOutputDirectory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/target/test-classes</testOutputDirectory>
    <resources>
      <resource>
        <directory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/gen</directory>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <directory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/src/test/resources</directory>
      </testResource>
    </testResources>
    <directory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/target</directory>
    <finalName>dilbert-1.8</finalName>
    <pluginManagement>
      <plugins>
        <plugin>
          <artifactId>maven-antrun-plugin</artifactId>
          <version>1.3</version>
        </plugin>
        <plugin>
          <artifactId>maven-assembly-plugin</artifactId>
          <version>2.2-beta-5</version>
        </plugin>
        <plugin>
          <artifactId>maven-dependency-plugin</artifactId>
          <version>2.1</version>
        </plugin>
        <plugin>
          <artifactId>maven-release-plugin</artifactId>
          <version>2.0</version>
        </plugin>
      </plugins>
    </pluginManagement>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.0</version>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-clean-plugin</artifactId>
        <version>2.4.1</version>
        <executions>
          <execution>
            <id>default-clean</id>
            <phase>clean</phase>
            <goals>
              <goal>clean</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-install-plugin</artifactId>
        <version>2.3.1</version>
        <executions>
          <execution>
            <id>default-install</id>
            <phase>install</phase>
            <goals>
              <goal>install</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-deploy-plugin</artifactId>
        <version>2.7</version>
        <executions>
          <execution>
            <id>default-deploy</id>
            <phase>deploy</phase>
            <goals>
              <goal>deploy</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-site-plugin</artifactId>
        <version>3.0</version>
        <executions>
          <execution>
            <id>default-site</id>
            <phase>site</phase>
            <goals>
              <goal>site</goal>
            </goals>
            <configuration>
              <outputDirectory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/target/site</outputDirectory>
              <reportPlugins>
                <reportPlugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-project-info-reports-plugin</artifactId>
                </reportPlugin>
              </reportPlugins>
            </configuration>
          </execution>
          <execution>
            <id>default-deploy</id>
            <phase>site-deploy</phase>
            <goals>
              <goal>deploy</goal>
            </goals>
            <configuration>
              <outputDirectory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/target/site</outputDirectory>
              <reportPlugins>
                <reportPlugin>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-project-info-reports-plugin</artifactId>
                </reportPlugin>
              </reportPlugins>
            </configuration>
          </execution>
        </executions>
        <configuration>
          <outputDirectory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/target/site</outputDirectory>
          <reportPlugins>
            <reportPlugin>
              <groupId>org.apache.maven.plugins</groupId>
              <artifactId>maven-project-info-reports-plugin</artifactId>
            </reportPlugin>
          </reportPlugins>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <reporting>
    <outputDirectory>/Users/mareksebera/Documents/PROJEKTY/eclipse2/SimpleDilbert/target/site</outputDirectory>
  </reporting>
</project>
